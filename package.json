{
  "name": "reason-libvterm",
  "version": "1.0.1",
  "description": "libvterm bindings for OCaml/ReasonML",
  "license": "MIT",
  "scripts": {
    "format": "esy dune build @fmt --auto-promote"
  },
  "esy": {
    "build": [
      [
        "dune",
        "build",
        "-p",
        "vterm"
      ]
    ],
    "install": [
      "esy-installer vterm.install"
    ]
  },
  "dependencies": {
    "@opam/dune": "*",
    "@opam/dune-configurator": "*",
    "@esy-ocaml/reason": "^3.3.7",
    "refmterr": "^3.1.0",
    "esy-libvterm": "^1.0.0",
    "reason-native-crash-utils": "github:onivim/reason-native-crash-utils#38c8f003"
  },
  "devDependencies": {
    "ocaml": "~4.8.0"
  }
}
